pullRequest:
  closed:
    releaseMe:
      targetBranch: 'master'
  opened:
    autoMerge:
      mergeCondition:
        whitelistedFiles:
          allowedFiles:
            'composer.lock':
              - modified
            'yarn.lock':
              - modified
            'package.lock':
              - modified
            '.gitignore':
              - modified
              - added